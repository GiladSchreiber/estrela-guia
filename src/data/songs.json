    "lyrics": [
      [
        { "pt": "Mandei caiar meu sobrado", "he": "שלחתי לסייד את הבית שלי" },
        { "pt": "Mandei, mandei, mandei", "he": "שלחתי, שלחתי, שלחתי" },
        { "pt": "Mandei caiar de amarelo", "he": "שלחתי לסייד בצהוב" },
        { "pt": "Caiei, caiei, caiei", "he": "סיידתי, סיידתי, סיידתי" }
      ],
      [
        { "pt": "Amarelo que lembra dourado", "he": "צהוב שמזכיר זהב" },
        { "pt": "Dourado, que é meu berimbau", "he": "זהב, שהוא הבירימבאו שלי" },
        { "pt": "Dourado, de cordão de ouro", "he": "זהב, כמו חוט זהב" },
        { "pt": "Besouro, Besouro, Besouro", "he": "ביזורו, ביזורו, ביזורו" }
      ],
      [
        { "pt": "Pra quem nunca ouviu falar", "he": "למי שמעולם לא שמע עליו" },
        { "pt": "Pra aqueles que dizem: é lenda", "he": "לאלה שאומרים: \"זה מיתוס\"" },
        { "pt": "Pois saibam que Besouro Preto", "he": "אז תדעו שביזורו השחור" },
        { "pt": "Viveu, viveu e morreu", "he": "חי, חי ומת" }
      ],
      [
        { "pt": "Pras bandas de Maracangalha", "he": "באזור של מרקנגליה" },
        { "pt": "Sem temer a inimigo nenhum", "he": "מבלי לפחד מאף אויב" },
        { "pt": "Não valeu, seu corpo fechado", "he": "הוא לא עמד, אפילו עם גופו הסגור" },
        { "pt": "Pras facas de aticum", "he": "מול הסכינים של אטיקום" }
      ],
      [
        { "pt": "Mas mesmo depois de morto", "he": "אבל גם אחרי מותו" },
        { "pt": "Entre uma e outra cantiga", "he": "בין שיר אחד לאחר" },
        { "pt": "Besouro vai sempre viver", "he": "ביזורו תמיד ימשיך לחיות" },
        { "pt": "Enquanto existir mandinga", "he": "כל עוד קיימת מנדינגה" }
      ]
